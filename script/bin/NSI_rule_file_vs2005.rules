<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="NSI"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="NSI"
			DisplayName="NSI compiler"
			CommandLine="$(ProjectDir)script\bin\nsis\makensis.exe  $(InputPath)"
			Outputs="$(InputName).exe"
			FileExtensions="*.nsi"
			ExecutionDescription="Begins NSI installer compiling..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
